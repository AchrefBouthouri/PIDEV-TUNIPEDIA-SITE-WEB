

{% block content %}
 <section class="w3ls-section contact py-5">
	<div class="container py-sm-3">
		<div class="w3ls-title">
			<h2 class="heading text-center mb-sm-5 mb-4">modify program</h2>
		</div>
	
		

		<div class="row contact_wthreerow agileits-w3layouts">
		
			<div class="col-lg-7 col-md-6 mt-md-0 mt-5 w3l_contact_form">
				<h4>program details</h4>
				{{ form_start(form,{'attr':{'novalidate': 'novalidate'}}) }}

				        
				    
						



                        <fieldset>
                        {{ form_row(form.name, {'attr':{'placeholder': 'name'}}) }}
						<div style="move-top:200px; ">
                    </fieldset>
						<div style="margin-top:20px; ">
                        <fieldset>
                        {{ form_row(form.date, {'attr':{'placeholder': 'date'}}) }}
						<div style="move-top:200px; ">
                    
					


                        <div class="col-md-100">
                    <fieldset>
                        {{ form_row(form.prix, {'attr':{'placeholder': 'prix'}}) }}
						<div style="move-top:200px; ">
                    </fieldset>
                </div>

				<div style="margin-top:20px; ">

						<div class="col-md-100">
                    <fieldset>
					    
					  				
                        {{ form_row(form.description, {'attr':{'placeholder': 'Description'}})  }}
						
						
                    </fieldset> 
                </div>
					    
						<div style="margin-top:20px; ">
						

						<button class="btn">{{ button_label|default('Save') }}</button>

       
                        
                       
                        {{ form_end(form) }}

						
                        </div>
			</div>
		</div>
	</div>
</section>
{% endblock %}